<?xml version="1.0" encoding="UTF-8"?>
<resource xmlns:xsi="http://www.w3.org/2001/XMLSchema" xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/rest-api 3.xsd" name="Party"  displayName="Party Rest api" version="2.0">
    <resource name="partyGet" require-authentication="anonymous-all">
        <id name="partyId">
            <method type="get">
                <entity name="Party" operation="one"/>
            </method>
        </id>

    </resource>

    <resource name="partyPost" require-authentication="anonymous-all">
        <method type="post">
<!--            <service name="TrainingServices.PartyService.create#Party"/>-->
            <entity name="Party" operation="create"/>
        </method>
    </resource>

    <resource name="partyPUT" require-authentication="anonymous-all">
        <id name="partyId">
            <method type="put">
                <entity name="Party" operation="update"/>
            </method>
        </id>
    </resource>
    <resource name="partyDelete" require-authentication="anonymous-all">
        <id name="partyId">
            <method type="delete">
<!--                <service name="TrainingServices.PartyService.delete#Party"/>-->
                <entity name="Party" operation="delete"/>
            </method>
        </id>
    </resource>


</resource>